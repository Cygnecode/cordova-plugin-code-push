            <header-file src="src/ios/JWT/Core/Algorithms/RSFamily/RSKeys/JWTCryptoKey.h" />
            <source-file src="src/ios/JWT/Core/Algorithms/RSFamily/RSKeys/JWTCryptoKey.m" />
